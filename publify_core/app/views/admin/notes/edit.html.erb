<% content_for :page_heading do %>
  <h2>
    <i class="glyphicon glyphicon-large glyphicon-th-list"></i>
    <%= t('.notes') %>
  </h2>
<% end %>


<div class='row'>
  <div class='col-md-4'>
    <%= form_for @note, url: admin_note_path(@note) do |n| %>
      <%= render 'form', n: n %>
    <% end %>
  </div>

  <%= render 'list' %>
</div>
